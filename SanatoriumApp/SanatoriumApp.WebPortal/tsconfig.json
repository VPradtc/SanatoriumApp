{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": true,
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "removeComments": false,
    "sourceMap": false,
    "target": "es2015",
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "allowJs": false,
    "alwaysStrict": true,
    "types": [
    ]
  },
  "include": [
    "wwwroot/**/*",
    "Scripts/typings/**/*"
  ],
  "exclude": [
    "wwwroot/lib/**/*",
    "Scripts/_references.js"
  ]
}
